{
  "name": "commander-gulp-scripts",
  "version": "1.0.3",
  "description": "Compilación de javascripts dinámicamente",
  "main": "./bin/scripts.js",
  "author": "Jesús Antonio Ugaz Vegas",
  "email": "jesusugazv@gmail.com",
  "license": "MIT",
  "bin": {
    "commander-gulp-scripts": "bin/scripts.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-plumber": "^1.2.1",
    "laravel-mix": "^5.0.6",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^3.0.2",
    "figlet": "^1.5.0",
    "graceful-fs": "^4.2.4",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-minify": "^3.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8"
  },
  "scripts": {
    "deploy": "npm run scripts",
    "dev": "npm run development",
    "scripts": "node ./bin/scripts.js scripts \"frontend/src/static/scripts/**/*.js\" --scr \"docs/scripts\"",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "preinstall": "node bin/pre-cli-scripts.js"
  },
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "scripts",
    "js",
    "gulp",
    "commander-gulp-scripts",
    "commander"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jugaz/commander-gulp-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/jugaz/commander-gulp-scripts/issues"
  },
  "homepage": "https://github.com/jugaz/commander-gulp-scripts#readme"
}
